  pod 'CocoaLumberjack/Swift'
  pod 'SnapKit'
  pod 'JVFloatLabeledTextField'
  pod 'CryptoSwift'
  pod 'NYTPhotoViewer', '~> 1.0.0'
  target 'EncryptQRTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
